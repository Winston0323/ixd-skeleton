    <link href="/css/mainpage.css" rel="stylesheet">
    <link href="/css/artist.css" rel="stylesheet">
</head>

<body>
    <nav class="navbar navbar-expand bg-light fixed-top shadow-sm">
        <a class="navbar-brand" href="#">
            <img class="home" id='lg' alt="Brand logo" src="/images/Oval.png">
        </a>
        <div>
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link active home" aria-current="page" href="http://localhost:3000">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Messages</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button"
                        data-bs-toggle="dropdown" aria-expanded="false">
                        Profile
                    </a>
                    <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <li><a class="dropdown-item" href="#">Log In</a></li>
                        <li><a class="dropdown-item" href="#">Sign Up</a></li>
                        <li>
                            <hr class="dropdown-divider">
                        </li>
                        <li><a class="dropdown-item" href="#">Log Out</a></li>
                    </ul>
                </li>
            </ul>
    </nav>
    <nav style="--bs-breadcrumb-divider: '>';" aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item home"><a href="http://localhost:3000">Home</a></li>
            <li class="breadcrumb-item active" aria-current="page">Artist</li>
        </ol>
    </nav>
    <section class="container-fluid">
        <div class="row" id="head">
            <div class="col-sm-2"></div>
            <figure class="col-sm-8">
                <img class="img-fluid" id="h1" src="/images/{{imageName}}" />
            </figure>
            <div class=" col-sm-2">
            </div>
        </div>
    </section>
    <br>
    <br>
    <ul class="nav nav-tabs">
        <li role="presentation" class="active"><a href="#">Description</a></li>
        <li role="presentation"><a href="#">Availability</a></li>
        <li role="presentation"><a href="#">Pricing</a></li>
    </ul>
    <div class="container-fluid" id="paragraph">
        <p class="container lead">
            <br>
            <br>
            {{artistName}}
            <br>
            <br>
        </p>
    </div>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
        integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
        crossorigin="anonymous"></script>
