<body>
    <nav class="navbar navbar-expand bg-light fixed-top shadow-sm">
        <a class="navbar-brand" href="#">
            <img class="home" id='lg' alt="Brand logo" src="/images/Oval.png">
        </a>
        <ul class="navbar-nav me-auto">
            <li class="nav-item">
                <a class="nav-link home" aria-current="page" href="http://localhost:3000/">Home</a>
            </li>
        </ul>
        <ul class="navbar-nav">
            <li class="nav-item dropdown">
                <a class=" nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button"
                    data-bs-toggle="dropdown" aria-expanded="false">
                    Profile
                </a>
                <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                    <li><a class="dropdown-item" href="http://localhost:3000/calendars">Schedules</a>
                    </li>
                    <li><a class="dropdown-item" href="http://localhost:3000/orders">Orders</a></li>
                    {{!-- <li><a class="dropdown-item disabled" href="#">Profile</a></li> --}}
